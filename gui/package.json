{
  "name": "gastown-gui",
  "version": "0.1.0",
  "description": "Web GUI for Gas Town multi-agent orchestrator",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js --dev",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.unit.config.js",
    "test:e2e": "vitest run test/e2e.test.js",
    "test:watch": "vitest watch",
    "test:ui": "vitest run --ui"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "puppeteer": "^21.6.1",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
